{
    "manifest_version": 3,
    "name": "GoPhish",
    "version": "0.2",
    "description": "Detecting malicious URLS with ML",
    "permissions": ["storage","scripting","notifications","contextMenus", "activeTab"],
    "action": {
        "default_title": "Collect URLs",
        "default_popup": "gophish.html"
    },
    "background":{
        "service_worker": "background.js"
    }
}